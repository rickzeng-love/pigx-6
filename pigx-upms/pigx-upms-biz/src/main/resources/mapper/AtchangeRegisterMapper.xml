<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pig4cloud.pigx.admin.mapper.AtchangeRegisterMapper">

  <resultMap id="atchangeRegisterMap" type="com.pig4cloud.pigx.admin.entity.AtchangeRegister">
                  <id property="id" column="ID"/>
                        <result property="term" column="Term"/>
                        <result property="badge" column="Badge"/>
                        <result property="eid" column="EID"/>
                        <result property="name" column="Name"/>
                        <result property="compid" column="CompID"/>
                        <result property="depid" column="DepID"/>
                        <result property="jobid" column="JobID"/>
                        <result property="lineidOld" column="LineID_Old"/>
                        <result property="teamidOld" column="TeamID_Old"/>
                        <result property="groupidOld" column="GroupID_Old"/>
                        <result property="aidOld" column="AID_Old"/>
                        <result property="cardidOld" column="CardID_Old"/>
                        <result property="atstatusOld" column="ATStatus_Old"/>
                        <result property="lineid" column="LineID"/>
                        <result property="teamid" column="TeamID"/>
                        <result property="groupid" column="GroupID"/>
                        <result property="aid" column="AID"/>
                        <result property="cardid" column="CardID"/>
                        <result property="atstatus" column="ATStatus"/>
                        <result property="begindate" column="BeginDate"/>
                        <result property="enddate" column="EndDate"/>
                        <result property="seqid" column="SeqID"/>
                        <result property="grpid" column="GrpID"/>
                        <result property="initialized" column="Initialized"/>
                        <result property="initializedby" column="InitializedBy"/>
                        <result property="initializedtime" column="InitializedTime"/>
                        <result property="submit" column="Submit"/>
                        <result property="submitby" column="SubmitBy"/>
                        <result property="submittime" column="SubmitTime"/>
                        <result property="closed" column="Closed"/>
                        <result property="closedby" column="ClosedBy"/>
                        <result property="closedtime" column="ClosedTime"/>
                        <result property="remark" column="Remark"/>
                        <result property="cardaddridOld" column="CardAddrID_Old"/>
                        <result property="cardaddrid" column="CardAddrID"/>
                        <result property="corpid" column="CorpID"/>
                        <result property="corpcode" column="CorpCode"/>
            </resultMap>

	<!--考勤变动确认校验-->
	<select id="aSPChangeCheck" statementType="CALLABLE" resultType="java.util.Map"    parameterType="java.util.HashMap" >

		{call aSP_ChangeCheck_Easy(#{id,mode=IN,jdbcType=INTEGER},#{userid,mode=IN,jdbcType=INTEGER},#{result,mode=OUT,jdbcType=INTEGER})}
	</select>

	<!--考勤变动开启-->
	<select id="aSPChangeStart" statementType="CALLABLE" resultType="java.util.Map"   parameterType="java.util.HashMap" >

		{call aSP_ChangeStart_Easy(#{id,mode=IN,jdbcType=INTEGER},#{userid,mode=IN,jdbcType=INTEGER},#{result,mode=OUT,jdbcType=INTEGER})}
	</select>
</mapper>
